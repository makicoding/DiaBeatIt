    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mdbreact": "^4.16.0",
    "mongoose": "^5.3.16",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6",
    "react-google-maps": "^9.4.5",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.0",
    "styled-components": "^4.3.2"
  }
